{
    "name": "cucumber-reportify",
    "version": "0.3.1",
    "description": "HTML reports for Cucumber test results",
    "main": "index.js",
    "types": "index.d.ts",
    "scripts": {
        "lint": "eslint src",
        "format": "prettier --write src",
        "format:check": "prettier --check src",
        "build": "tsc && tsc-alias -p tsconfig.json",
        "bundle": "npx rollup -c",
        "test": "jest ."
    },
    "author": {
        "name": "Lucien Lu",
        "email": "lucienlu2000@gmail.com"
    },
    "license": "ISC",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/marutsuki/cucumber-reportify.git"
    },
    "keywords": [
        "cucumber",
        "report",
        "html",
        "performance",
        "merge",
        "json",
        "test"
    ],
    "devDependencies": {
        "@eslint/js": "8.57.0",
        "@rollup/plugin-commonjs": "^26.0.1",
        "@rollup/plugin-node-resolve": "^15.2.3",
        "@rollup/plugin-terser": "^0.4.4",
        "@types/jest": "^29.5.12",
        "@types/mustache": "^4.2.5",
        "@types/node": "^20.14.12",
        "@types/stream-json": "^1.7.7",
        "@types/yargs": "^17.0.32",
        "eslint": "8.57.0",
        "globals": "^15.8.0",
        "jest": "^29.7.0",
        "prettier": "3.3.3",
        "rollup": "^4.19.2",
        "rollup-plugin-copy": "^3.5.0",
        "rollup-plugin-peer-deps-external": "^2.2.4",
        "ts-jest": "^29.2.3",
        "tsc-alias": "^1.8.10",
        "typescript": "^5.5.4",
        "typescript-eslint": "^7.17.0"
    },
    "dependencies": {
        "mustache": "^4.2.0",
        "stream-json": "^1.8.0",
        "yargs": "^17.7.2"
    },
    "optionalDependencies": {
        "@rollup/rollup-linux-x64-gnu": "4.9.5"
    },
    "bin": {
        "cuke": "cli.js"
    }
}
